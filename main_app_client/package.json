  "scripts": {
    "build": "webpack --mode production --progress && rm -rfv ./dist/static && cp -rfv ./src/static/ ./dist/static/ && test -s ./dist/static/json/locales/en.json && exit 0 || exit 1",
    "build-p": "webpack -p --progress --optimize-minimize --config webpack.config.production.js",
    "build-demo": "webpack --mode production --progress --env target=remote_demo --env apiPort=443 && rm -rfv ./dist/static && cp -rfv ./src/static/ ./dist/static/ && mv ./dist/static/json/locales ./dist/static/json/qc_demo_locales && test -s ./dist/static/json/qc_demo_locales/en.json && exit 0 || exit 1",
    "test": "jest --runInBand --verbose --config jest.config.js",
    "test-s": "./test/runtest.sh",
    "start": "webpack serve --mode development --config webpack.config.development.js --hot --inline --progress --host 0.0.0.0 --port 3000 --open",
    "start-hpe": "webpack serve --mode development --config webpack.config.development.js --env target=hpe --hot --inline --progress --host localhost --port 3000 --open",
    "start-adlink": "webpack serve --mode development --config webpack.config.development.js --env target=adlink --hot --inline --progress --host localhost --port 3000 --open",
    "start-no-open": "webpack serve --mode development --config webpack.config.development.js --hot --inline --progress --host 0.0.0.0 --port 3000",
    "eslint": "npx eslint --fix --no-error-on-unmatched-pattern -c .eslintrc.js --ext .ts,.tsx,.js,.jsx --ignore-pattern *.html ./src/**",
    "clear_jest": "jest --clearCache"
  },
